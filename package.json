    "@strapi/sdk-plugin": "^5.2.6",
    "@strapi/strapi": "^5.0.5",
    "eslint": "^8.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "chartbrew",
